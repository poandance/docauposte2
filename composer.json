{
    "name": "kiloutyg/docauposte2",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "symfony/monolog-bundle": "^3.10",
        "doctrine/orm": "^3.3.2",
        "picqer/php-barcode-generator": "^3.2",
        "symfony/apache-pack": "^1.0.1",
        "symfony/cache": "^7",
        "symfony/console": "^7",
        "symfony/dotenv": "^7",
        "symfony/error-handler": "^7",
        "symfony/flex": "^2.4.7",
        "symfony/form": "^7",
        "symfony/framework-bundle": "^7",
        "symfony/http-foundation": "^7",
        "symfony/mailer": "^7",
        "symfony/mime": "^7",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/runtime": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "^7",
        "symfony/ux-turbo": "^2.23",
        "symfony/validator": "^7",
        "symfony/var-dumper": "^7",
        "symfony/web-profiler-bundle": "^7",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "^7",
        "tecnickcom/tcpdf": "^6.8.2",
        "tomasvotruba/barcode-bundle": "^1.5",
        "twig/twig": "^3.20|^3.20"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.0",
            "docker": false
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.62.1",
        "symfony/process": "^7",
        "symfony/stopwatch": "^7"
    }
}
